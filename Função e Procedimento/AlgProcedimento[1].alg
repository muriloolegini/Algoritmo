algoritmo "semnome"
// Autor : Murilo Olegini
// Data : 20/10/2016

var
numero1,numero2,somaNumero:real

procedimento soma
inicio
somaNumero <- numero1+numero2
escreva ("O total da soma:",somaNumero)
fimprocedimento

inicio
escreva ("Digite o primeiro número: ")
leia (numero1)
escreva ("Digite o segundo número: ")
leia (numero2)
soma()

fimalgoritmo
